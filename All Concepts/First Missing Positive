class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int ans=0;
        set<int> s;
        for(int i=0;i<nums.size();i++){
            s.insert(nums[i]);
        }
        for(int i=1;i<100005;i++){
            auto it = s.find(i);
            
            if(it != s.end()){
                continue;
            } else {
                cout<<"ans: "<<i<<endl;
                ans=i;
                break;
            }
        }
        return ans;
    }
};